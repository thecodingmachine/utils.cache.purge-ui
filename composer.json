{
    "name": "mouf/utils.cache.purge-ui",
    "description": "This package contains a user interface that will add a 'Purge cache' button to Mouf, allowing you to purge any PSR-6, PSR-16, Doctrine and Mouf cache easily.",
    "keywords": ["mouf", "cache", "psr-6", "psr-16", "doctrine"],
    "homepage": "https://github.com/thecodingmachine/utils.cache.purge-ui",
    "type": "mouf-library",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com",
            "homepage": "http://mouf-php.com"
        }
    ],
    "require": {
    	"php": ">=5.5",
        "mouf/utils.composite-exception": "~1.0"
    },
    "require-dev": {
      "psr/cache" : "^1.0",
      "psr/simple-cache" : "^1.0",
      "doctrine/cache": "^1.4",
      "mouf/utils.cache.cache-interface": "^2"
    },
    "autoload": {
        "psr-4": {
            "Mouf\\Utils\\Cache\\Purge\\": "src/Mouf/Utils/Cache/Purge"
        }
    },
    "extra": {
    	"mouf": {
    		"require-admin": [
    			"CacheAdmin.php"
    		],
    		"logo": "logo.png"
    	}
    }
}
